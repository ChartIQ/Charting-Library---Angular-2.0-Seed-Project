{
  "name": "chartiq-angular2",
  "version": "1.0.0",
  "scripts": {
    "start": "lite-server",
    "prestart": "npm run compile",
    "compile": "tsc",
    "compile_watch": "tsc -w",
    "postinstall": "typings install & npm run compile",
    "debug": "concurrent lite-server \"npm run compile_watch\"",
    "precompile": "copy index.html dist && copy app/**/*.html dist && copy app/**/*.css dist && copy img/* dist && copy app/css/* dist"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "~3.1.0",
    "@angular/upgrade": "~2.1.0",
    "@types/core-js": "^0.9.35",
    "angular2": "2.0.0-beta.7",
    "copy": "^0.3.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rimraf": "^2.5.4",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.22",
    "typescript": "^1.8.10",
    "typings": "^2.1.0",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0"
  }
}
